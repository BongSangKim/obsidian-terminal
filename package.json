{
	"name": "obsidian-terminal",
	"displayName": "Obsidian Terminal",
	"version": "3.1.0",
	"author": "polyipseity",
	"license": "AGPL-3.0-or-later",
	"description": "Integrate consoles, shells, and terminals inside Obsidian.",
	"keywords": [
		"console",
		"obsidian",
		"plugin",
		"shell",
		"terminal",
		"terminal emulator"
	],
	"private": true,
	"main": "main.js",
	"scripts": {
		"build": "tsc -noEmit && npm run lint && npm run dev production",
		"check": "tsc --noEmit",
		"dev": "node build/esbuild.config.mjs",
		"lint": "eslint --cache --ignore-pattern \"*.svelte\" .",
		"lint:fix": "eslint --cache --fix --ignore-pattern \"*.svelte\" .",
		"postversion": "node build/version-commit.mjs",
		"version": "node build/version-bump.mjs && git add manifest.json versions.json"
	},
	"dependencies": {
		"ansi-escape-sequences": "6.2.1",
		"deep-equal": "2.2.0",
		"i18next": "22.4.10",
		"i18next-resources-to-backend": "1.1.1",
		"json-stringify-safe": "5.0.1",
		"lodash": "4.17.21",
		"monkey-around": "2.3.0",
		"semver": "7.3.8",
		"simple-icons": "8.5.0",
		"tmp": "0.2.1",
		"ts-essentials": "9.3.0",
		"xterm": "5.1.0",
		"xterm-addon-canvas": "0.3.0",
		"xterm-addon-fit": "0.7.0",
		"xterm-addon-ligatures": "0.6.0",
		"xterm-addon-search": "0.11.0",
		"xterm-addon-serialize": "0.9.0",
		"xterm-addon-unicode11": "0.5.0",
		"xterm-addon-web-links": "0.8.0",
		"xterm-addon-webgl": "0.14.0"
	},
	"devDependencies": {
		"@tsconfig/node16-strictest": "1.0.4",
		"@types/ansi-escape-sequences": "4.0.0",
		"@types/deep-equal": "1.0.1",
		"@types/json-stringify-safe": "5.0.0",
		"@types/lodash": "4.14.191",
		"@types/node": "16.18.12",
		"@types/tmp": "0.2.3",
		"@typescript-eslint/eslint-plugin": "5.52.0",
		"@typescript-eslint/parser": "5.52.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.9",
		"esbuild-svelte": "0.7.3",
		"eslint-import-resolver-typescript": "3.5.3",
		"eslint-plugin-import": "2.27.5",
		"eslint-plugin-svelte3": "4.0.0",
		"obsidian": "file:build/obsidian",
		"svelte": "3.55.1",
		"svelte-preprocess": "5.0.1",
		"tslib": "2.5.0",
		"typescript": "4.9.5",
		"typescript-svelte-plugin": "0.3.14"
	},
	"obsidian": {
		"id": "terminal",
		"name": "Terminal",
		"minAppVersion": "1.1.13",
		"authorUrl": "",
		"fundingUrl": {},
		"isDesktopOnly": false
	}
}