{
	"name": "obsidian-terminal",
	"displayName": "Obsidian Terminal",
	"version": "2.9.0",
	"author": "polyipseity",
	"license": "MIT",
	"description": "Integrate consoles/shells/terminals inside Obsidian.",
	"keywords": [
		"console",
		"obsidian",
		"plugin",
		"shell",
		"terminal",
		"terminal emulator"
	],
	"private": true,
	"main": "main.js",
	"scripts": {
		"dev": "node build/esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node build/esbuild.config.mjs production",
		"version": "node build/version-bump.mjs && git add manifest.json versions.json",
		"postversion": "node build/version-commit.mjs"
	},
	"dependencies": {
		"i18next": "22.4.9",
		"tmp": "0.2.1",
		"xterm": "5.1.0",
		"xterm-addon-fit": "0.7.0",
		"xterm-addon-search": "0.11.0",
		"xterm-addon-serialize": "0.9.0",
		"xterm-addon-web-links": "0.8.0"
	},
	"devDependencies": {
		"@tsconfig/node16-strictest": "^1.0.4",
		"@types/node": "^16.18.11",
		"@types/tmp": "^0.2.3",
		"@typescript-eslint/eslint-plugin": "5.48.2",
		"@typescript-eslint/parser": "5.48.2",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.4",
		"obsidian": "^1.1.1",
		"tslib": "2.4.1",
		"typescript": "4.9.4"
	},
	"obsidian": {
		"id": "terminal",
		"name": "Terminal",
		"minAppVersion": "1.1.9",
		"authorUrl": "",
		"fundingUrl": {},
		"isDesktopOnly": false
	}
}