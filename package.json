{
	"name": "obsidian-terminal",
	"displayName": "Obsidian Terminal",
	"version": "2.11.0",
	"author": "polyipseity",
	"license": "MIT",
	"description": "Integrate consoles, shells, and terminals inside Obsidian.",
	"keywords": [
		"console",
		"obsidian",
		"plugin",
		"shell",
		"terminal",
		"terminal emulator"
	],
	"private": true,
	"main": "main.js",
	"scripts": {
		"build": "tsc -noEmit && npm run lint && npm run dev production",
		"dev": "node build/esbuild.config.mjs",
		"lint": "eslint --ignore-pattern \"*.svelte\" --rule \"@typescript-eslint/no-unsafe-argument: off\" --rule \"@typescript-eslint/no-unsafe-assignment: off\" --rule \"@typescript-eslint/no-unsafe-return: off\" --rule \"@typescript-eslint/restrict-template-expressions: off\" .",
		"lint:fix": "eslint --fix --ignore-pattern \"*.svelte\" --rule \"@typescript-eslint/no-unsafe-argument: off\" --rule \"@typescript-eslint/no-unsafe-assignment: off\" --rule \"@typescript-eslint/no-unsafe-return: off\" --rule \"@typescript-eslint/restrict-template-expressions: off\" .",
		"postversion": "node build/version-commit.mjs",
		"version": "node build/version-bump.mjs && git add manifest.json versions.json"
	},
	"dependencies": {
		"i18next": "22.4.9",
		"simple-icons": "8.4.0",
		"tmp": "0.2.1",
		"xterm": "5.1.0",
		"xterm-addon-canvas": "0.3.0",
		"xterm-addon-fit": "0.7.0",
		"xterm-addon-ligatures": "0.6.0",
		"xterm-addon-search": "0.11.0",
		"xterm-addon-serialize": "0.9.0",
		"xterm-addon-unicode11": "0.5.0",
		"xterm-addon-web-links": "0.8.0",
		"xterm-addon-webgl": "0.14.0"
	},
	"devDependencies": {
		"@tsconfig/node16-strictest": "1.0.4",
		"@types/node": "16.18.11",
		"@types/tmp": "0.2.3",
		"@typescript-eslint/eslint-plugin": "5.50.0",
		"@typescript-eslint/parser": "5.50.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.5",
		"esbuild-svelte": "0.7.3",
		"eslint-import-resolver-typescript": "3.5.3",
		"eslint-plugin-import": "2.27.5",
		"eslint-plugin-svelte3": "4.0.0",
		"obsidian": "1.1.1",
		"svelte": "3.55.1",
		"svelte-preprocess": "5.0.1",
		"tslib": "2.5.0",
		"typescript": "4.9.5",
		"typescript-svelte-plugin": "0.3.14"
	},
	"obsidian": {
		"id": "terminal",
		"name": "Terminal",
		"minAppVersion": "1.1.9",
		"authorUrl": "",
		"fundingUrl": {},
		"isDesktopOnly": false
	}
}